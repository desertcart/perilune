%div{id: "ddd"}
  :javascript
    Highcharts.chart('ddd', {
      chart: {
        type: 'areaspline',
        height: 500
      },
      title: { text: '' },
      xAxis: {
        type: 'datetime',
        dateTimeLabelFormats: { // don't display the year
           month: '%e. %b',
           year: '%b'
         },
         title: {
            text: 'Date'
         }
      },
      yAxis: {
        labels: {
          format: '{value}%'
        },
        title: {
          text: 'Percentage',
        }
      },
      plotOptions: {
        series: {
        },
        areaspline: {
          stacking: 'percent',
          marker: {
            enabled: false
          }
        }
      },
      series: []
    });
